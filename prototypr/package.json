{
	"name": "prototypr",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint --fix",
		"pages:build": "npx @cloudflare/next-on-pages",
		"preview": "npm run pages:build && wrangler pages dev",
		"deploy": "npm run pages:build && wrangler pages deploy",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate:local": "wrangler d1 migrations apply https-secsuite-db --local",
		"db:migrate:preview": "wrangler d1 migrations apply --env preview https-secsuite-db --remote",
		"db:migrate:prod": "wrangler d1 migrations apply https-secsuite-db --remote"
	},
	"dependencies": {
		"@auth0/nextjs-auth0": "^3.5.0",
		"@aws-sdk/client-s3": "^3.665.0",
		"@aws-sdk/s3-request-presigner": "^3.665.0",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-slot": "^1.1.0",
		"@react-pdf-viewer/core": "^3.12.0",
		"@react-pdf-viewer/default-layout": "^3.12.0",
		"@react-pdf/renderer": "^4.0.0",
		"axios": "^1.7.7",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.33.0",
		"formidable": "^3.5.1",
		"lucide-react": "^0.447.0",
		"next": "14.2.5",
		"next-auth": "^4.24.8",
		"openai": "^4.67.1",
		"react": "^18",
		"react-dom": "^18",
		"react-resizable-panels": "^2.1.4",
		"tailwind-merge": "^2.5.3",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@cloudflare/next-on-pages": "^1.13.5",
		"@cloudflare/workers-types": "^4.20241004.0",
		"@tailwindcss/typography": "^0.5.15",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"drizzle-kit": "^0.24.2",
		"eslint": "^8",
		"eslint-config-next": "14.2.5",
		"eslint-plugin-next-on-pages": "^1.13.5",
		"postcss": "^8",
		"tailwindcss": "^3.4.1",
		"typescript": "^5",
		"vercel": "^37.6.1",
		"wrangler": "^3.80.0"
	}
}
